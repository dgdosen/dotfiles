{
  "title": "Tmux Window Navigation",
  "rules": [
    {
      "description": "Cmd+Shift+[ to tmux previous window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["left_command", "left_shift"]
            }
          },
          "to": [
            {
              "key_code": "a",
              "modifiers": ["left_command"]
            },
            {
              "key_code": "p"
            }
          ]
        }
      ]
    },
    {
      "description": "Cmd+Shift+] to tmux next window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "mandatory": ["left_command", "left_shift"]
            }
          },
          "to": [
            {
              "key_code": "a",
              "modifiers": ["left_command"]
            },
            {
              "key_code": "n"
            }
          ]
        }
      ]
    }
  ]
}
